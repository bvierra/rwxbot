extends layout

block body
  div.channels
    ul
      - each channel in channels
        li
          a(href= '/logs/' + encode(channel) + '/latest') #{channel}
          = ' ('
          a(href= '/logs/' + encode(channel) + '/index') index
          = ' | '
          a(href= '/logs/' + encode(channel) + '/latest') latest
          = ' | '
          a(href= '/logs/' + encode(channel) + '/urls') URLS
          = ')'
